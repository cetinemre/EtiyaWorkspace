package oop2;

public class Logger {
	public void log(String data, int logType) {
		if(logType==1) {
			System.out.println(data+" dosyaya logland覺");
		}else if(logType==2) {
			System.out.println(data+"db logland覺");
		}
		else if(logType==3) {
			System.out.println(data+"cloud logland覺");
		}else if(logType==4){
			System.out.println(data+"elastic logland覺");
		}
	}
	
}
