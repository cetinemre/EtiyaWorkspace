
public class GercekMusteri extends Musteri{
	private int musteriId;
	private String ad;
	private String soyad;
	private String TcKimlikNo;	
	private TicariFaaliyet ticariFaaliyet;
	public GercekMusteri(int id, int musteriNo, int kanalId) {
		super(id, musteriNo, kanalId);
	}
	public GercekMusteri(int id, int musteriNo, int kanalId, int musteriId, String ad, String soyad,
			String tcKimlikNo) {
		super(id, musteriNo, kanalId);
		this.musteriId = musteriId;
		this.ad = ad;
		this.soyad = soyad;
		TcKimlikNo = tcKimlikNo;
	}
	
}
