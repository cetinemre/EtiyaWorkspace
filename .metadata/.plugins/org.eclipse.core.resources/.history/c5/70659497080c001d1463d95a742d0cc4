
public class GercekMusteri extends Musteri{
	private int MusteriId;
	private String Ad;
	private String Soyad;
	private String TcKimlikNo;	
	private TicariFaaliyet ticariFaaliyet;
	public GercekMusteri(int musteriId, String ad, String soyad, String tcKimlikNo) {
		super();
		MusteriId = musteriId;
		Ad = ad;
		Soyad = soyad;
		TcKimlikNo = tcKimlikNo;
	}
	public int getMusteriId() {
		return MusteriId;
	}
	public void setMusteriId(int musteriId) {
		MusteriId = musteriId;
	}
	public String getAd() {
		return Ad;
	}
	public void setAd(String ad) {
		Ad = ad;
	}
	public String getSoyad() {
		return Soyad;
	}
	public void setSoyad(String soyad) {
		Soyad = soyad;
	}
	public String getTcKimlikNo() {
		return TcKimlikNo;
	}
	public void setTcKimlikNo(String tcKimlikNo) {
		TcKimlikNo = tcKimlikNo;
	}
	public TicariFaaliyet getTicariFaaliyet() {
		return ticariFaaliyet;
	}
	public void setTicariFaaliyet(TicariFaaliyet ticariFaaliyet) {
		this.ticariFaaliyet = ticariFaaliyet;
	}
	
}
